{
  "version": "0.1.0",
  "description": "A snap that checks for recurring transactions",
  "proposedName": "Recurring transaction checker",
  "repository": {
    "type": "git",
    "url": "https://github.com/MetaMask/template-snap-monorepo.git"
  },
  "source": {
    "shasum": "pu4yYPy/XMj83Q2KuctZEi508C98C/Yg54SDivb1u7I=",
    "location": {
      "npm": {
        "filePath": "dist/bundle.js",
        "iconPath": "images/icon.svg",
        "packageName": "snap",
        "registry": "https://registry.npmjs.org/"
      }
    }
  },
  "initialPermissions": {
    "snap_dialog": {},
    "snap_notify": {},
    "snap_manageState": {},
    "endowment:rpc": {
      "dapps": true,
      "snaps": false
    },
    "endowment:ethereum-provider": {},
    "endowment:cronjob": {
      "jobs": [
        {
          "expression": "* * * * *",
          "request": {
            "method": "everyMinute",
            "params": {
              "param1": "foo"
            }
          }
        }
      ]
    },
    "endowment:transaction-insight": {
      "allowTransactionOrigin": true
    }
  },
  "manifestVersion": "0.1"
}
